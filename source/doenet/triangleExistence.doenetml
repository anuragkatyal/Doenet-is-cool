<paginator name="pgn">

<!--*************************************************************
          Page 1 - 3,4,5
**************************************************************-->
  
<div>
<graph xmin="-1" xmax="20" ymin="-1" ymax="20" size="large" displayXAxis="false" displayYAxis="false">
<group name="sideAGroup">
  <point name="p1" styleNumber="5" showCoordsWhenDragging="false">(3,3)
    <constraints>
      <attractTo>$p3</attractTo>
      <attractTo>$p4</attractTo>
      <attractTo>$p5</attractTo>
      <attractTo>$p6</attractTo>
    </constraints>
  </point>
  <point name="p2" styleNumber="2" showCoordsWhenDragging="false">(8,3) 
    <constraints>
      <constrainTo>$lengthConstraintA</constrainTo>
    </constraints>
  </point>
  <circle name="lengthConstraintA" center="$p1" radius="3" styleNumber="6"/>
  <lineSegment name="sideA" endpoints="$p1 $p2" applyStyleToLabel>
    <label><m>A</m></label>
  </lineSegment>
</group>


<group name="sideBGroup">
  <point name="p3" styleNumber="5" showCoordsWhenDragging="false">(15,6)
    <constraints>
      <attractTo>$p5</attractTo>
      <attractTo>$p6</attractTo>
    </constraints>
  </point>
  <point name="p4" styleNumber="3" showCoordsWhenDragging="false">(15,10) 
    <constraints>
      <constrainTo>$lengthConstraintB</constrainTo>
    </constraints>
  </point>
  <circle name="lengthConstraintB" center="$p3" radius="4" styleNumber="6"/>
  <lineSegment name="sideB" endpoints="$p3 $p4" styleNumber="3" applyStyleToLabel>
    <label><m>B</m></label>
  </lineSegment>
</group>


<group name="sideCGroup">
  <point name="p5" styleNumber="5" showCoordsWhenDragging="false">(8,14)</point>
  <point name="p6" styleNumber="4" showCoordsWhenDragging="false">(13,14) 
    <constraints>
      <constrainTo>$lengthConstraintC</constrainTo>
    </constraints>
  </point>
  <circle name="lengthConstraintC" center="$p5" radius="5" styleNumber="6"/>
  <lineSegment name="sideC" endpoints="$p5 $p6" styleNumber="4" applyStyleToLabel>
    <label><m>C</m></label>
  </lineSegment>
</group>

<legend>
  <label forObject="$sideA"><m>A = $sideA.length</m></label>
  <label forObject="$sideB"><m>B = $sideB.length</m></label>
  <label forObject="$sideC"><m>C = $sideC.length</m></label>
</legend>
</graph>

<p>How can you tell?</p>
<answer  handGraded expanded type="text" forceFullCheckworkButton/>

</div>

