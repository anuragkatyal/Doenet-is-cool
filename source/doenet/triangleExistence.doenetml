<!--*************************************************************
          Page 5 - answer questions given triples
**************************************************************-->
<div newNamespace>

<p>What about the side lengths given below?</p>

<ul>
  <li> <!--a true triangle-->
    <p><m>3, 6, 7</m>
    <booleanInput name="biT1" disabled="$biF1"><label>T</label></booleanInput>
    <booleanInput name="biF1" disabled="$biT1"><label>F</label></booleanInput>
    </p>
  </li>
  <li>
    <p><m>1, 4, 2</m> <!--a false triangle-->
    <booleanInput name="biT2" disabled="$biF2"><label>T</label></booleanInput>
    <booleanInput name="biF2" disabled="$biT2"><label>F</label></booleanInput>
    </p>
  </li>
  <li>
    <p><m>9, 4, 6</m> <!--a true triangle-->
    <booleanInput name="biT3" disabled="$biF3"><label>T</label></booleanInput>
    <booleanInput name="biF3" disabled="$biT3"><label>F</label></booleanInput>
    </p>
  </li>
  <li>
    <p><m>3, 3, 1</m> <!--a true triangle-->
    <booleanInput name="biT4" disabled="$biF4"><label>T</label></booleanInput>
    <booleanInput name="biF4" disabled="$biT4"><label>F</label></booleanInput>
    </p>
  </li>
  <li>
    <p><m>3, 2, 1</m> <!--a false triangle-->
    <booleanInput name="biT5" disabled="$biF5"><label>T</label></booleanInput>
    <booleanInput name="biF5" disabled="$biT5"><label>F</label></booleanInput>
    </p>
  </li>
</ul>

<p><booleanList name="responses">$biT1 $biT2 $biT3 $biT4 $biT5</booleanList></p>
<p><booleanList name="solutions">true false true true false</booleanList></p>
  
<answer matchPartial matchByExactPositions>
  <award><when>$responses = $solutions</when></award>
</answer>
